{
    "name": "0x_react_setup",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "lint:eslint": "eslint",
        "format:check": "prettier --check .",
        "format:fix": "prettier --write .",
        "lint:stylelint": "stylelint **/*.css",
